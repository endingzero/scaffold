<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjw.scaffold.mapper.UserMapper">


    <select id="listAllUser" resultType="com.zjw.scaffold.response.UserExportResponse">
        select id,
        username,
        name
        from user
        where status = '1'
    </select>
</mapper>
